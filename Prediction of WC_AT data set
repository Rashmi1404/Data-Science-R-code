#Load Data
WC_AT <- read.csv("C:/Users/Rashmi/Desktop/ExcelR/R/WC_AT.csv")
View(WC_AT)

#Visualisation
library(lattice)
dotplot(WC_AT$AT,main="Dot Plot of AT", col="Black")
dotplot(WC_AT$Waist,main="Dot Plot of Waist", col="Blue")
boxplot(WC_AT$AT,col="Pink")
boxplot(WC_AT$Waist,col="Red")

#Finding Y
model<-lm(AT~Waist,data=WC_AT)
summary(model)

#finding y^ predicting y
new<-data.frame(Waist=c(60,70))
pred<-predict(model,newdata = new)
pred

#finding Error 
pred<-predict(model)
finaldata<-data.frame(WC_AT,pred,"Error"= WC_AT$Waist-pred)
finaldata
